# uses regular expression syntax
syntax: regexp

# ignores main binaries
^bin/

# ignores lib binaries
^lib/libal/bin/
^lib/libbullet/bin/
^lib/libjpeg/bin/
^lib/liblua/bin/
^lib/libogg/bin/
^lib/libpng/bin/
^lib/libtheora/bin/
^lib/libvorbis/bin/
^lib/libzlib/bin/

# ignores visual studio 2008 working directories
^win32/vs2008ex/hive_mariachi/Debug/
^win32/vs2008ex/hive_mariachi/Debug \(Dynamic\)/
^win32/vs2008ex/hive_mariachi/Release/
^win32/vs2008ex/hive_mariachi/Release \(Dynamic\)/
^win32/vs2008ex/hive_mariachi_runner/Debug/
^win32/vs2008ex/hive_mariachi_runner/Debug \(Dynamic\)/
^win32/vs2008ex/hive_mariachi_runner/Release/
^win32/vs2008ex/hive_mariachi_runner/Release \(Dynamic\)/
^win32/vs2008ex/lib_\w+/Debug/
^win32/vs2008ex/lib_\w+/Debug \(Dynamic\)/
^win32/vs2008ex/lib_\w+/Release/
^win32/vs2008ex/lib_\w+/Release \(Dynamic\)/

# ignores visual studio 2008 auxiliary files
^win32/vs2008ex/.*.ncb$
^win32/vs2008ex/.*.suo$

# ignores visual studio user files
^win32/vs2008ex/hive_mariachi\.vcproj\..*\..*.user
^win32/vs2008ex/hive_mariachi_runner\.vcproj\..*\..*.user
^win32/vs2008ex/lib_\w+\.vcproj\..*\..*.user

# ignores xcode working directories
^darwin/xcode/build/
^darwin/xcode_iphone/build/

# ignores xcode local files
\.mode1v3$
\.pbxuser$

# ignores windows metadata files
Thumbs\.db

# ignores mac os x metadata files
\.DS_Store
